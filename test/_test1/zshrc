# ffmpeg
ff_tomp4() {
    if [ "$2" != "" ]; then
        ffmpeg -i "$1" -qscale 0 "$2"
    else
        ffmpeg -i "$1" -qscale 0 "${1%.*}.mp4"
    fi
}

