# Convert iso to dmg
x1iso2dmg() {
    hdiutil convert -format UDRW -o "${1%.iso}".dmg "${1}"
}
g1zip() {
    f=`basename $(pwd)`-`date +%Y%m%d` ; rm -vf "$f".zip ; git archive --output "$f".zip --prefix "$f/" master ; du "$f".zip
}

