export A="1"

# HELPERS {{{1
myls()
{
    [ "$PWD" != "${PWD%Dropbox*}" ] && echo -e '\x1b[5m<Dropbox>\x1b[0m'
    if [ ! -d _darcs ]; then
        if [ ! -d .git ]; then
            # Normal
            command ls "$@"
            return
        fi
    fi

    if [ "$PWD" = "$HOME" ]; then
        # In $HOME - still normal
        command ls "$@"
        return
    fi

    # Full listing if in a git/darcs repository
    command ls -A "$@"
}
# 1}}}

