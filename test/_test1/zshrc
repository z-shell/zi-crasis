n1gglinks() {
    search=`echo "$1+$2+$3+$4+$5+$6+$7+$8+$9" | sed 's:\+*$::'`
    links google.com/search\?q="$search"
}
n1ggw3m() {
    search=`echo "$1+$2+$3+$4+$5+$6+$7+$8+$9" | sed 's:\+*$::'`
    w3m google.com/search\?q="$search"
}
n1ling() {
    search=`echo "$1+$2+$3+$4+$5+$6+$7+$8+$9" | sed 's:\+*$::'`
    w3m ling.pl/"$search"
}
n1diki() {
    search=`echo "$1+$2+$3+$4+$5+$6+$7+$8+$9" | sed 's:\+*$::'`
    w3m www.diki.pl/slownik-angielskiego/"?q=$search"
}
n1dict() {
    search=`echo "$1+$2+$3+$4+$5+$6+$7+$8+$9" | sed 's:\+*$::'`
    w3m dict.pl/dict"?word=$search"
}

